theory WideMouthedFrog
begin

builtins : hashing, symmetric-encryption
macros: hashedxor(x, y, z) = h(x ⊕ y ⊕ z),
        coucou(x) = bonjour(x)

lemma key_secrecy_2:
    "not(Ex A B S kab #i #j.
        Server_send(hashedxor(A, S, B) kab) @i
        
    )
    "
end