theory WideMouthedFrog
begin

builtins : hashing, symmetric-encryption
functions: blabla/2


rule Alice_1:
    [
        !Secretk($A, $S, %3)
        ,Fr(~kab)
        ,Fr(~Ta)
    ]
  --[Alice_sends($A, $S, ~kab)]->
    [
      Out(<$A,senc(<' 1', ~Ta, $B, ~kab>,kas)>)
    ]

end